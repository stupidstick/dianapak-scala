<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.dianapak.view.ListPointController">
    <children>
        <VBox>
            <VBox>
                <Button mnemonicParsing="false" text="insert" onAction="#insert"/>
                <Button mnemonicParsing="false" text="insert by pos" onAction="#insertByPos"/>
                <Button mnemonicParsing="false" text="remove" onAction="#remove"/>
                <Button mnemonicParsing="false" text="sort" onAction="#sort"/>
                <Button mnemonicParsing="false" text="serialize" onAction="#serialize"/>
                <Button mnemonicParsing="false" text="deserialize" onAction="#deserialize"/>
            </VBox>
            <VBox>
                <HBox>
                    <TextField fx:id="xField"/>
                    <Label text="x"/>
                </HBox>
                <HBox>
                    <TextField fx:id="yField"/>
                    <Label text="y"/>
                </HBox>
            </VBox>
        </VBox>
        <ListView layoutX="316.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0" fx:id="listView"/>
    </children>
</AnchorPane>
